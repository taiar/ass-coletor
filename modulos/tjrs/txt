<script>
function startList() {
	//code only for IE
	if(!document.body.currentStyle) return;
	ajustaScrollHoriz();
	
	//no scroll ie...
	//document.body.scroll="vertical";
	//document.body.scrollWidth = 800;
	//document.body.scrollHeight = 556;

	var subs = document.getElementsByName('submenu');
	for(var i=0; i<subs.length; i++) {
		var li = subs[i].parentNode;
		if(li && li.lastChild.style) {
			li.onmouseover = function() {
				this.lastChild.style.visibility = 'visible';
			}
			li.onmouseout = function() {
				this.lastChild.style.visibility = 'hidden';
			}
		}
	}

	//var ul1 = document.getElementsById('menuList').getElementsByTag('');
}

ajustaScrollHoriz = function() {
/*    //alert(document.body.clientWidth);
	if(!document.body.currentStyle) {
		alert('nao eh IE!');
		return;
	}*/
/*	alert('style='+getActiveStyleSheet());
	if("tam-1"!=getActiveStyleSheet()) {
		document.body.style.overflowX='auto'; return;
	}*/

	if("tam-1"==getActiveStyleSheet()) {
		if(document.body.clientWidth>=778) {
			document.body.style.overflowX='hidden';
		}
		else {
			document.body.style.overflowX='auto';
		}
	}
	else {
		document.body.style.overflowX='auto'; return;
	}
}

//window.onload=startList;
window.onresize=ajustaScrollHoriz; 
</script>

